<section class="instagram-feed max-wrap">
  <h2 class="instagram-feed__title">
    <a href="{{ section.settings.url }}">{{ section.settings.title }}</a>
  </h2>
  <div id="insta-feed"></div>
</section>

{% schema %}
  {
    "name": "Testimonials",
    "settings": [
      {
        "type": "text",
        "id": "title",
        "label": "Title"
      },
      {
        "type": "url",
        "id": "link",
        "label": "Link"
      }
    ],
    "presets": [{
        "name": "Instagram",
        "category": "Front Page"
    }]
  }
{% endschema %}
